{
  "type": "pt",
  "classifiers": [
    {
      "type": "place"
    },
    {
      "type": "transition"
    }
  ],
  "relations": [
    {
      "type": "arc",
      "bind": {
        "place": [
          "transition"
        ],
        "transition": [
          "place"
        ]
      },
      "arrow-end": "arrow-head"
    }
  ],
  "arrow-heads": [
    {
      "type": "arrow-head"
    }
  ],
  "texts": [
    {
      "type": "name",
      "targets": [
        "place",
        "transition",
        "arc"
      ]
    },
    {
      "type": "marking",
      "regex": "\\d+",
      "default": "1",
      "targets": [
        "place"
      ]
    }
  ]
}

